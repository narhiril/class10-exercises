@{
}

<h1>Events:</h1>
<p>
    <a asp-controller="Events" asp-action="Add">Add Event</a>
</p>

<div>
@if(ViewBag.events?.Count > 0 ?? false)
{
    <ul>
    @foreach (string ev in ViewBag.events){
        <li>@ev</li>
    }
    </ul>
}
else
{
    <p>No events found!</p>
}
</div>